<?xml version="1.0" encoding="utf-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>cadastrarPergunta</managed-bean-name>
  <managed-bean-class>br.com.bean.CadastrarPergunta</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>perguntas</managed-bean-name>
  <managed-bean-class>br.com.service.Perguntas</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>trocarTema</managed-bean-name>
  <managed-bean-class>br.com.bean.TrocarTema</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>nomeConectividade</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>tema</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tableBean</managed-bean-name>
  <managed-bean-class>remove.TableBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>usuarioMB</managed-bean-name>
  <managed-bean-class>br.com.service.UsuarioMBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>usuario</managed-bean-name>
  <managed-bean-class>br.com.bean.Usuario</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>administradorMB</managed-bean-name>
  <managed-bean-class>br.com.service.AdministradorMBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>administrador</managed-bean-name>
  <managed-bean-class>br.com.bean.Administrador</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>quizRealizadaMB</managed-bean-name>
  <managed-bean-class>br.com.service.QuizRealizadaMB</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>quizRealizada</managed-bean-name>
  <managed-bean-class>br.com.bean.QuizRealizada</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>cadastrarPergunta</property-name>
   <property-class>br.com.bean.CadastrarPergunta</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>chartBean</managed-bean-name>
  <managed-bean-class>br.com.bean.ChartBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>alunoMB</managed-bean-name>
  <managed-bean-class>br.com.service.AlunoMB</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>page/login.xhtml</display-name>
  <from-view-id>/page/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/page/apresentacao.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <validator>
  <validator-id>EmailValidator</validator-id>
  <validator-class>br.com.service.EmailValidator</validator-class>
 </validator>
 <validator>
  <validator-id>KeyValidator</validator-id>
  <validator-class>start.KeyValidator</validator-class>
 </validator>
 <application>
  <message-bundle>start.message</message-bundle>
 </application>
</faces-config>
